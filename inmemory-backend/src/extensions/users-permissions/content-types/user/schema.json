{
  "collectionName": "users",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3
    },
    "email": {
      "type": "email",
      "required": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "inversedBy": "users"
    },
    "votes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vote.vote",
      "mappedBy": "user"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "user"
    }
  }
} 